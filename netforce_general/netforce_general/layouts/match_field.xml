<form model="match.field" title_field="title">
    <!--<head>-->
    <!--<field name="state"/>-->
    <!--</head>-->
    <field name="file" onchange="onchange_file" nolabel="1"/>
    <newline/>
    <group span="6">
        <separator string="Match field"/>
        <field name="lines" nolabel="1" noadd="1" noremove="1" count="0">
            <list>
                <field name="customer_field" readonly="1"/>
                <field name="field_id" onchange="onchange_line" condition='[["model","=",parent.model]]'/>
            </list>
        </field>
    </group>
    <group span="6" attrs='{"invisible": [["state","!=","error"]]}'>
        <separator string="Error Message"/>
        <field name="log_lines" nolabel="1" noadd="1" noremove="1" count="0">
            <list>
                <field name="sequence"/>
                <field name="description"/>
            </list>
        </field>
    </group>
    <field name="model" invisible="1"/>
    <field name="next" invisible="1"/>
    <field name="state" invisible="1"/>
    <foot replace="1">
        <button string="Verify" method="do_verify" type="success"/>
        <button string="Import" method="do_import" type="primary"/>
    </foot>
</form>
