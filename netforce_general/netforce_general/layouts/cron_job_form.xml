<form model="cron.job">
    <field name="name"/>
    <field name="date"/>
    <field name="model"/>
    <field name="method"/>
    <field name="args"/>
    <newline/>
    <field name="interval_num"/>
    <field name="interval_type"/>
    <field name="call_num"/>
    <field name="timeout"/>
    <field name="state"/>
    <separator/>
    <field name="last_error_time"/>
    <field name="error_message"/>
    <related>
        <field name="comments"/>
    </related>
</form>
